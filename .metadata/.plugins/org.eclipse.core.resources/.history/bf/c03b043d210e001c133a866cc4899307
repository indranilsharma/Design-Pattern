package com.designPatern.StructuralDesignPatterns.DecoratorPattern;

interface Shape {
	String info();
}

class circle implements Shape {

	private float radius;

	public circle() {
	}

	public circle(float radius) {
		this.radius = radius;
	}

	void resize(float factor) {
		radius *= factor;
	}

	@Override
	public String info() {
		return null;
	}

}

public class B_Dynamic_Decorator {

}
